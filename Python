import pandas as pd
import matplotlib.pyplot as plt
hours_studies = [1,3,6,7,8,9,10]
exam_scores = [20,47,78,88,90,94,98.6]
plt.scatter(hours_studies,exam_scores,color ='green',marker='d',label = 'Student Data')
plt.title("Score with Hours of Studies Data  ")
plt.xlabel("HOURS STUDIES")
plt.ylabel("EXAM MARK")
plt.legend()
plt.grid()
plt.show()